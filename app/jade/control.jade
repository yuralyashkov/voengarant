include layout/_headerU
body
    .admin-panel
    header.header
        .top
            .top-line
                .container-left
                    a(href="/")
                        img(src="./images/logo.png")
                .container.container__header
                    nav
                        ul
                            - var TopList = ['Новости', 'Информация', 'Форум', 'Отзывы', 'Калькуляторы', 'Аналитика', 'Объединение', 'Инициативы', 'Партнеры', 'Контакты']
                            for item in TopList
                                li
                                    a(href='#')= item

                            li.header__search-btn
                                a(href="javascript:void(0)")
                                    img(src="./images/icons/search.png")

                .container-right
                    a(href="javascript:void(0)")
                        img(src="./images/icons/user.png")
                        span Вход
        .header__navigation
            .top-line
                .container-left
                    a(href="javascript:void(0)")
                        img(src="./images/icons/navigation_badge.png")
                        span Регформа
                    a(href="javascript:void(0)")
                        img(src="./images/icons/navigation_badge.png")
                        span Заявка в банк
                .container.container__navigation
                    .row.container__navigation__row
                        .col-md-6
                            .row
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon1.png")
                                        span Новостройки
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon3.png")
                                        span Эксклюзив
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon2.png")
                                        span Застройщики
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon4.png")
                                        span Банки
                        .col-md-6
                            .row
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon5.png")
                                        span Ипотека повторно
                                .col-md-3
                                    a(href="#")
                                        img(src="./images/icons/navigation_menu_icon6.png")
                                        span Вопрос юристу
                                .col-md-3
                                    a(href="#")
                                        img.interaction(src="./images/icons/navigation_menu_icon7.png")
                                        span Взаимодействие
                                .col-md-3
                                    a(href="#")
                                        img.interaction(src="./images/icons/navigation_menu_icon8.png")
                                        span Контроль

                .container-right
                    a(href="#").phone 8 (800) 500-43-44
                    a(href="#").phone 8 (495) 150-31-71

include layout/control/_control
include layout/control/_control-stage

include layout/_footer
